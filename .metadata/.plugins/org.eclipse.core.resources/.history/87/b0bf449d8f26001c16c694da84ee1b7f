package auxiliares;

public class Deporte {
	private int id;
	private String nombre;
	
	public Deporte(String nombre) {
		this.id = -1;
	}
	
	public Deporte(int id, String nombre) {
		this.id = id;
		this.nombre = nombre;
	}

	@Override
	public String toString() {
		return id + ", " + nombre;
	}
	
	
}
