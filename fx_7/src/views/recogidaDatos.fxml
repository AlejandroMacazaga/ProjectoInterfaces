              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="btnAceptar" mnemonicParsing="false" onAction="#aceptar" text="%button.aceptar" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                  <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#cancelar" text="%button.cancelar" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
               </children>
            </GridPane>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </GridPane>
